{
    "name": "kduma/sms-driver-justsend",
    "description": "An JustSend.pl driver for kduma/sms package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krystian Duma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "illuminate/support": "^5.6",
        "kduma/sms": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "KDuma\\SMS\\Drivers\\JustSend\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KDuma\\SMS\\Drivers\\JustSend\\JustSendServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
